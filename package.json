{
  "name": "tron-web3",
  "version": "1.0.6",
  "description": "Tron has ready-made methods to connect to wallets and perform many transactions.",
  "targets": {
    "frontend": {
      "source": "tron-web3.js"
    },
    "module": false,
    "main": false,
    "types": false
  },
  "scripts": {
    "serve": "parcel test.html --no-cache --dist-dir test",
    "build": "parcel build --no-cache --no-source-maps --dist-dir dist",
    "build-w": "parcel watch --no-cache --no-source-maps --dist-dir dist"
  },
  "main": "tron-web3.js",
  "browser": "dist/tron-web3.js",
  "module": "dist/tron-web3.js",
  "types": "tron-web3.js.d.ts",
  "typings": "tron-web3.js",
  "prepublishOnly": "npm run build",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BeycanDeveloper/tron-web3-js.git"
