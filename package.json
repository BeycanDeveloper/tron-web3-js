  "bugs": {
    "url": "https://github.com/BeycanDeveloper/tron-web3-js/issues"
  },
  "homepage": "https://github.com/BeycanDeveloper/tron-web3-js",
  "dependencies": {
    "@beycandeveloper/tron-tx-decoder": "^2.0.4",
    "bignumber.js": "^9.0.2"
  },
  "devDependencies": {
    "@parcel/transformer-vue": "^2.3.2",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0"
  }
}
